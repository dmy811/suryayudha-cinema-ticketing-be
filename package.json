{
  "name": "surya-yudha-e-ticket-cinema-booking-backend",
  "version": "1.0.0",
  "main": "./build/server/index.js",
  "scripts": {
    "prebuild": "rimraf build",
    "build:watch": "tsc-watch --onSuccess \"tsc-alias\"",
    "dev": "cross-env NODE_ENV=development nodemon --delay 2000ms",
    "build": "tsc && tsc-alias && pnpm copy-docs",
    "copy-docs": "copyfiles -u 1 src/docs/** build",
    "validate:yaml": "swagger-cli validate ./src/docs/openapi.yaml",
    "prod": "cross-env NODE_ENV=production node ./build/server/index.js",
    "test": "cross-env NODE_ENV=test vitest --run --reporter verbose --coverage",
    "test:watch": "cross-env NODE_ENV=test vitest --watch --reporter verbose",
    "test:ci": "cross-env NODE_ENV=test vitest --run --coverage --reporter=verbose --reporter=junit --reporter=json",
    "test:ui": "cross-env NODE_ENV=test vitest --ui",
    "format": "prettier . --write",
    "check-types": "tsc --noEmit --pretty",
    "check-format": "prettier . --check",
    "gen:private:accesstoken": "mkdir -p keys && openssl genrsa -out keys/accessTokenPrivateKey.pem 2048",
    "gen:public:accesstoken": "mkdir -p keys && openssl rsa -in keys/accessTokenPrivateKey.pem -pubout -out src/keys/accessTokenPublicKey.pem",
    "gen:private:accesstoken:b64": "base64 -w0 keys/accessTokenPrivateKey.pem > keys/accessTokenPrivateKey.b64",
    "gen:public:accesstoken:b64": "base64 -w0 keys/accessTokenPublicKey.pem > keys/accessTokenPublicKey.b64",
    "gen:private:refreshtoken:b64": "base64 -w0 keys/refreshTokenPrivateKey.pem > keys/refreshTokenPrivateKey.b64",
    "gen:public:refreshtoken:b64": "base64 -w0 keys/refreshTokenPublicKey.pem > keys/refreshTokenPublicKey.b64",
    "prisma:migrate:dev": "cross-env NODE_ENV=development prisma migrate dev --schema=./src/infrastructure/database/prisma/schema.prisma",
    "prisma:migrate:test": "cross-env NODE_ENV=test prisma migrate deploy --schema=./src/infrastructure/database/prisma/schema.prisma",
    "prisma:migrate:prod": "cross-env NODE_ENV=production prisma migrate deploy --schema=./src/infrastructure/database/prisma/schema.prisma",
    "prisma:generate:dev": "cross-env NODE_ENV=development prisma generate --schema=./src/infrastructure/database/prisma/schema.prisma",
    "prisma:generate:test": "cross-env NODE_ENV=test prisma generate --schema=./src/infrastructure/database/prisma/schema.prisma",
    "prisma:generate:prod": "cross-env NODE_ENV=production prisma generate --schema=./src/infrastructure/database/prisma/schema.prisma",
    "prisma:seed:dev": "cross-env NODE_ENV=development node ./build/infrastructure/database/seed.js",
    "prisma:seed:test": "cross-env NODE_ENV=test tsx ./src/infrastructure/database/seed.ts",
    "prisma:seed:prod": "cross-env NODE_ENV=production node ./build/infrastructure/database/seed.js",
    "prisma:studio:dev": "cross-env NODE_ENV=development prisma studio --schema=./src/infrastructure/database/prisma/schema.prisma"
  },
  "keywords": [
    "cinema",
    "movies",
    "e-ticket",
    "booking-app",
    "backend"
  ],
  "author": "Dimas Mukhtar Yuliawan",
  "license": "ISC",
  "description": "Backend of the Surya Yudha Cinema e-ticket booking application, enabling users to browse movies, select showtimes, choose seats, and purchase tickets online.",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "@types/cors": "^2.8.17",
    "argon2": "^0.41.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "envalid": "^8.0.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "imagekit": "^6.0.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.4.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "rate-limiter-flexible": "^9.0.1",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yamljs": "^0.3.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.14.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-facebook": "^3.0.4",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "@vitest/coverage-v8": "4.0.10",
    "@vitest/ui": "4.0.16",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.1.0",
    "nodemon": "^3.1.9",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.5.3",
    "prisma": "^6.6.0",
    "rimraf": "^6.1.0",
    "supertest": "^7.1.4",
    "tsc-alias": "^1.8.16",
    "tsc-watch": "^7.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3",
    "vitest": "^4.0.10"
  },
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
}
